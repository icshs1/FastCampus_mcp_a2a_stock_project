# ==============================
# Required - LLM & Core Services
# ==============================

# OpenAI
OPENAI_API_KEY=

# Kiwoom Securities (REST)
KIWOOM_APP_KEY=
KIWOOM_APP_SECRET=
KIWOOM_ACCOUNT_NO=

# Naver Search API
NAVER_CLIENT_ID=
NAVER_CLIENT_SECRET=

# Tavily Search API
TAVILY_API_KEY=

# Financial Data APIs
DART_API_KEY=
FRED_API_KEY=
ECOS_API_KEY=


# ==============================
# Optional - Runtime Toggles
# ==============================

# Logging & Debug
LOG_LEVEL=INFO
DEBUG=false

# Run modes
IS_DOCKER=false
MOCK_MODE=true

# Dev container bind mode for source volume (ro|rw)
# Used by docker-compose.full.yml volumes with ${DEV_MODE:-ro}
DEV_MODE=ro


# ==============================
# Optional - Additional Providers
# ==============================

# Other LLMs (optional)
ANTHROPIC_API_KEY=

# Market data providers (optional)
FINNHUB_API_TOKEN=


# ==============================
# Optional - A2A Credentials (if enabling A2A auth)
# ==============================

A2A_BEARER_TOKEN=
A2A_API_KEY=
A2A_OAUTH2_CLIENT_ID=
A2A_OAUTH2_CLIENT_SECRET=


# ==============================
# Optional - Advanced Logging (MCP utils)
# ==============================

ENVIRONMENT=development
LOG_MASKING_LEVEL=partial
ENABLE_LOG_FILTERING=true
LOG_FILTERING_PERFORMANCE_MODE=false


# ==============================
# Compatibility (not required)
# ==============================

# Some validators accept this as an alternative name; primary is KIWOOM_ACCOUNT_NO
KIWOOM_ACCOUNT_NUMBER=

## LangSmith
LANGSMITH_TRACING=true
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=default